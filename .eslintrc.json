{
  "extends": ["prettier", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "env": {
    "commonjs": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "operator-linebreak": 0,
    "import/extensions": 0,
    "import/prefer-default-export": 0,
    "comma-dangle": 0,
    "object-curly-newline": 0,
    "func-names": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "singleQuote": true,
        "printWidth": 100,
        "tabWidth": 2,
        "semi": true
      }
    ]
  },
  "plugins": ["prettier"]
}
